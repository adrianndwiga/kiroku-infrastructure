@Lambda
submitStatement 

@Queue
submitStatementQueue

@Store
submitStatementStore

@Lambda
transactionsUpdate

@Queue
transactionsQueue

@Store
transactionsStore

@Queue
salesAndExpencesReportQueue

@Store
salesAndExpencesReportStore

@Lambda
salesAndExpencesReport

@Cloud
azure

@Cloud
aws

@AccessRoles
submitStatement
submitStatementQueue
submitStatementStore
transactionsUpdate
transactionsQueue
transactionsStore
salesAndExpencesReport
salesAndExpencesReportQueue
salesAndExpencesReportStore

@AccessRights
submitStatement 
    canWrite:submitStatementQueue
    canWrite:submitStatementStore
submitStatementQueue
    allowWrite:submitStatement
    allowReadAndWrite:transactionsUpdate
submitStatementStore
    allowWrite:submitStatement
    allowRead:transactionsUpdate
transactionsUpdate
    canReadAndWrite:submitStatementQueue
    canRead:submitStatementStore
    canWrite:salesAndExpencesReportQueue
    canReadAndWrite:transactionsStore
transactionsStore
    allowRead:transactionsUpdate
    allowWrite:transactionsUpdate
salesAndReport
    canRead:salesAndExpencesReportStore
    canWrite:salesAndExpencesReportStore
salesAndExpencesReportStore
    allowRead:salesAndReport
    allowWrite:salesAndReport

if(cloud === 'azure') {
    /// create azure function {name}
} else if (cloud === 'aws') {
    /// create aws lambda {name}
}







